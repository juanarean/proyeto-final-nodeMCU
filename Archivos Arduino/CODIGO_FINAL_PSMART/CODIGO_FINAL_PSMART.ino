word shiftIn(int DataPin, int ClockPin) {
  int i;
  int temp = 0;
  int pinState;
  word DataIn = 0;
  for (i = 15; i >= 0; i--)
  {
    digitalWrite(ClockPin, 0);
    delayMicroseconds(0.2);
    temp = digitalRead(DataPin);
    if (temp) {
      pinState = 1;
      //set the bit to 0 no matter what
      DataIn = DataIn | (1 << i);
    }
    else {
   pinState = 0;
    }
 digitalWrite(ClockPin, 1);

  }

  return DataIn;
}
